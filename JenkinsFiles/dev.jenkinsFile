pipeline {
   agent any

   environment {
      BRANCH_NAME = 'master'
      input_version = '1.0.0'
      registry = "docker_hub_account/repository_name"
      registryCredential = 'dockerhub'
   }
   stages {
      stage('Pull code') {
         steps {
            git branch: 'master',credentialsId: 'github.com',url: 'git@github.com:moomdate/SUT-REG-API.git'
         }
      }
      stage('Build container image') {
         steps{
             sh 'whoami'
         }
      }
      stage('view dir and build') {
         steps {
            sh 'ls'
            sh 'docker build .'
         }
      }
   }
}
